@model ICollection<GetABuddy.Web.ViewModels.Event.CategoryViewModel>
@{
    ViewBag.Title = "_CategoryDropDown";
}

<h2>_CategoryDropDown</h2>

<select class="form-control col-md-4" name="CategoryId">
	@foreach (var category in Model)
	{
		<option value=@category.Id>@category.Name</option>
	}
</select>
